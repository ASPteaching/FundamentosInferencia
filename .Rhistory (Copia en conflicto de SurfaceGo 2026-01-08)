pretty_kable <- function(
x,
digits = NULL,
col.names = NULL,
align = NULL,
format = "html",
caption = NULL
) {
stopifnot(is.data.frame(x))
knitr::kable(
x,
digits = digits,
col.names = col.names,
align = align,
caption = caption,
format = format,
escape = FALSE
) |>
kableExtra::kable_styling(
position = "center",
full_width = FALSE
)
}
